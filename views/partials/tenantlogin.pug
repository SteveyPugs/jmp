#TenantModal.modal.fade(tabindex="-1" role="dialog" aria-hidden="true")
  .modal-dialog.modal-dialog-centered.modal-sm(role="document")
    .modal-content
      .modal-header
        h5#TenantModalLabel.modal-title Tenant Login
      .modal-body
        form.needs-validation(novalidate method="post" action="/login/Tenant")
          .form-group
            label(for="TenantEmail") E-Mail
            input#TenantEmail.form-control.form-control-sm(type="email" placeholder="name@example.com" name="TenantEmail" required)
            .invalid-feedback Please enter a valid e-mail
          .form-group
            label(for="TenantPassword") Password
            input#TenantPassword.form-control.form-control-sm(type="password" name="TenantPassword" required)
            .invalid-feedback Please enter a password
          button.btn.btn-dark.btn-block.btn-sm(type="submit") Sign In